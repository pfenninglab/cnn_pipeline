#!/bin/bash
#SBATCH --t = 8:00:00
#SBATCH --nodes=1
#SBATCH --gres=gpu:1
#SBATCH -o logs/out.txt
#SBATCH -e logs/err.txt
#SBATCH --mem 8000

# If training env isn't already activated, then activate it
if [ "$CONDA_DEFAULT_ENV" != "keras2-tf27" ]; then
	source activate keras2-tf27
fi

sweep_id=$1

wandb login
wandb agent --count 1 $sweep_id
